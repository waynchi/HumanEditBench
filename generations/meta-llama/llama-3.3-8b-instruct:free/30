def find_sums_of_four_squares():
    limits = 100
    result = []
    for a in range(1, limits):
        for b in range(a, limits):
            for c in range(b, limits):
                for d in range(c, limits):
                    if a**2 + b**2 + c**2 + d**2 < limits**2:
                        result.append((a**2 + b**2 + c**2 + d**2))
                    else:
                        break
    return result

result = find_sums_of_four_squares()
print(result)