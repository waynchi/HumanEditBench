def expressible_as_sum_of_four_nonzero_squares(n):
    # Tenta encontrar inteiros positivos a, b, c, d tais que
    # n = a^2 + b^2 + c^2 + d^2
    # Note que a, b, c, d >= 1 (não nulos)
    limit = int(n**0.5) + 1
    for a in range(1, limit):
        a2 = a*a
        if a2 > n:
            break
        for b in range(1, limit):
            b2 = b*b
            if a2 + b2 > n:
                break
            for c in range(1, limit):
                c2 = c*c
                if a2 + b2 + c2 > n:
                    break
                for d in range(1, limit):
                    d2 = d*d
                    total = a2 + b2 + c2 + d2
                    if total == n:
                        return True
                    elif total > n:
                        break
    return False

if __name__ == '__main__':
    # Lista números até um limite que podem ser expressos como a soma de 4 quadrados não nulos.
    limite = 100
    resultados = []
    for num in range(1, limite + 1):
        if expressible_as_sum_of_four_nonzero_squares(num):
            resultados.append(num)
    
    print("Números que podem ser expressos como a soma de quatro quadrados não nulos:")
    print(resultados)