def is_prime(n):
    """
    检查一个数是否为质数
    
    参数:
    n (int): 需要检查的数
    
    返回:
    bool: 如果n是质数返回True，否则返回False
    """
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True