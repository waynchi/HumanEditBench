def can_be_expressed_as_sum_of_four_nonzero_squares(n):
    """
    Verifica se um número pode ser expresso como a soma de quatro quadrados não nulos.
    
    Args:
        n (int): O número a ser verificado.
    
    Returns:
        bool: True se o número pode ser expresso como a soma de quatro quadrados não nulos, False caso contrário.
    """
    # Tentamos todas as combinações possíveis de quatro quadrados não nulos
    for a in range(1, int(n**0.5) + 1):
        a_squared = a * a
        if a_squared >= n:
            break
            
        for b in range(1, int((n - a_squared)**0.5) + 1):
            b_squared = b * b
            if a_squared + b_squared >= n:
                break
                
            for c in range(1, int((n - a_squared - b_squared)**0.5) + 1):
                c_squared = c * c
                if a_squared + b_squared + c_squared >= n:
                    break
                    
                # Verificamos se o número restante é um quadrado perfeito não nulo
                d_squared = n - a_squared - b_squared - c_squared
                d = int(d_squared**0.5)
                if d > 0 and d * d == d_squared:
                    return True
    
    return False

# Encontra e imprime os números que podem ser expressos como a soma de quatro quadrados não nulos
if __name__ == "__main__":
    count = 0
    for num in range(1, 101):
        if can_be_expressed_as_sum_of_four_nonzero_squares(num):
            print(f"{num} pode ser expresso como a soma de quatro quadrados não nulos")
            count += 1
    
    print(f"Total: {count} números entre 1 e 100 podem ser expressos como a soma de quatro quadrados não nulos")