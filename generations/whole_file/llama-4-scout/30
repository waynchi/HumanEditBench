def numeros_soma_quatro_quadrados():
    count = 0
    for i in range(1, 1000):
        for j in range(1, 1000):
            for k in range(1, 1000):
                for l in range(1, 1000):
                    soma = i**2 + j**2 + k**2 + l**2
                    if soma not in numeros and soma < 1000:
                        numeros.append(soma)

    return sorted(list(set(numeros)))

