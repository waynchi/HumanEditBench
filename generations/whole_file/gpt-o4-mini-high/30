import math

def eh_soma_quatro_quadrados_nao_nulos(n):
    # Apenas mudou o intervalo para começar em 1, garantindo quadrados não nulos
    for a in range(1, int(math.isqrt(n)) + 1):
        for b in range(1, int(math.isqrt(n)) + 1):
            for c in range(1, int(math.isqrt(n)) + 1):
                for d in range(1, int(math.isqrt(n)) + 1):
                    if a*a + b*b + c*c + d*d == n:
                        return True
    return False

def main():
    limite = int(input("Digite o limite máximo: "))
    resultados = []
    for numero in range(1, limite + 1):
        if eh_soma_quatro_quadrados_nao_nulos(numero):
            resultados.append(numero)
    print("Números que podem ser expressos como a soma de quatro quadrados não nulos:", resultados)

if __name__ == "__main__":
    main()