def binary_search(arr, target):
    """
    Бинарный поиск в отсортированном списке.
    
    Args:
        arr (list): Отсортированный список.
        target: Элемент, который нужно найти.
    
    Returns:
        int: Индекс элемента, если он найден, иначе -1.
    """
    low = 0
    high = len(arr) - 1
    
    while low <= high:
        mid = (low + high) // 2
        if arr[mid] == target:
            return mid
        elif arr[mid] < target:
            low = mid + 1
        else:
            high = mid - 1
    
    return -1

# Пример использования
arr = [2, 5, 8, 12, 16, 23, 38, 56, 72, 91]
target = 23
result = binary_search(arr, target)

if result != -1:
    print(f"Элемент {target} найден по индексу {result}")
else:
    print(f"Элемент {target} не найден в списке")