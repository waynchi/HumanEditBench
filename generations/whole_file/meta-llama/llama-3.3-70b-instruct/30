def soma_de_quatro_quadrados(n):
    """
    Verifica se um número pode ser expresso como a soma de quatro quadrados não nulos.
    
    Args:
    n (int): O número a ser verificado.
    
    Returns:
    bool: True se o número pode ser expresso como a soma de quatro quadrados não nulos, False caso contrário.
    """
    for a in range(1, int(n**0.5) + 1):
        for b in range(1, int(n**0.5) + 1):
            for c in range(1, int(n**0.5) + 1):
                for d in range(1, int(n**0.5) + 1):
                    if a**2 + b**2 + c**2 + d**2 == n:
                        return True
    return False

# Teste
num = 30
if soma_de_quatro_quadrados(num):
    print(f"{num} pode ser expresso como a soma de quatro quadrados não nulos.")
else:
    print(f"{num} não pode ser expresso como a soma de quatro quadrados não nulos.")