import time
import os

options = uc.ChromeOptions()
current_dir = os.path.dirname(__file__)
chromedriver_path = os.path.join(current_dir, "chromedriver-win64", "chromedriver.exe")
options.binary_location = chromedriver_path
print("wde")
with uc.Chrome(use_subprocess=True, options=options) as driver:
    print("wde")
    driver.get("https://lmarena.ai/")
    print("wde")
    # 为undetected_chromedriver创建ChromeOptions实例
    # 使用指定选项初始化未检测的Chrome驱动程序
    time.sleep(10)