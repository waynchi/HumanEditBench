def is_sum_of_four_squares(n):
    for a in range(1, int(n**0.5) + 1):
        for b in range(a, int((n - a**2)**0.5) + 1):
            for c in range(b, int((n - a**2 - b**2)**0.5) + 1):
                d = int((n - a**2 - b**2 - c**2)**0.5)
                if d >= c and a**2 + b**2 + c**2 + d**2 == n:
                    return True
    return False

def main():
    count = 0
    for i in range(1, 10000):
        if is_sum_of_four_squares(i):
            count += 1
    print(f"Total numbers that can be expressed as the sum of four non-zero squares: {count}")

if __name__ == "__main__":
    main()