# Implementation to find numbers that can be expressed as a sum of four non-zero squares

def is_sum_of_four_squares(n):
    if n < 1:
        return False
    count = 0
    for a in range(1, int(n**0.5) + 1):
        for b in range(1, int(n**0.5) + 1):
            for c in range(1, int(n**0.5) + 1):
                for d in range(1, int(n**0.5) + 1):
                    if a*a + b*b + c*c + d*d == n:
                        count += 1
                        if count > 0:
                            return True
    return False

# Example usage:
for i in range(1, 100):
    if is_sum_of_four_squares(i):
        print(f"{i} can be expressed as a sum of four non-zero squares.")