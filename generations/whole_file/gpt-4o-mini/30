# Implementation to find numbers that can be expressed as the sum of four non-zero squares

def can_be_expressed_as_four_squares(n):
    # Iterate through all possible combinations of four squares
    for a in range(1, int(n**0.5) + 1):
        for b in range(1, int(n**0.5) + 1):
            for c in range(1, int(n**0.5) + 1):
                for d in range(1, int(n**0.5) + 1):
                    if a**2 + b**2 + c**2 + d**2 == n:
                        return True
    return False

# Test the function with a range of numbers
for i in range(1, 100):
    if can_be_expressed_as_four_squares(i):
        print(f"{i} pode ser expresso como a soma de quatro quadrados não nulos.")
    else:
        print(f"{i} não pode ser expresso como a soma de quatro quadrados não nulos.")