# Números que podem ser expressos como a soma de quatro quadrados não nulos

def pode_ser_soma_quatro_quadrados_nao_nulos(n):
    """
    Verifica se um número pode ser expresso como soma de quatro quadrados não nulos
    """
    if n < 4:
        return False
    
    # Verifica se o número pode ser escrito como soma de 4 quadrados não nulos
    # Usando o teorema de Lagrange e considerações adicionais
    for a in range(1, int(n**0.5) + 1):
        if a*a >= n:
            break
        for b in range(a, int((n-a*a)**0.5) + 1):
            if a*a + b*b >= n:
                break
            for c in range(b, int((n-a*a-b*b)**0.5) + 1):
                d_squared = n - a*a - b*b - c*c
                if d_squared <= 0:
                    break
                d = int(d_squared**0.5)
                if d*d == d_squared and d > 0:
                    return True
    return False

def numeros_com_4_quadrados_nao_nulos(limit):
    """
    Retorna uma lista dos números até o limite que podem ser expressos
    como soma de quatro quadrados não nulos
    """
    result = []
    for i in range(4, limit + 1):
        if pode_ser_soma_quatro_quadrados_nao_nulos(i):
            result.append(i)
    return result

# Exemplo de uso
limit = 100
numeros = numeros_com_4_quadrados_nao_nulos(limit)
print(f"Números até {limit} que podem ser expressos como soma de quatro quadrados não nulos:")
print(numeros)