import os
from pymilvus import connections

# 记录当前路径为cwd
# 创建向量数据库路径
cwd = os.getcwd()
db_path = os.path.join(cwd, 'milvus_db.db')
TABLE_NAME = 'test_table'
DIM_VALUE = 128
# 使用 pymilvus 建立数据库连接
client = connections.connect(
    alias="default",
    host="localhost",
    port="19530"
)

# ...其余代码保持不变...