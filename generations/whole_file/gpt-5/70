from main13 import knn, mlp
import pandas as pd

np_df = pd.DataFrame(index=range(10, 200, 10), columns=[1, 2, 3], dtype=object)

for pclass in [1, 2, 3]:
    for fare in range(10, 200, 10):
            my_df = pd.DataFrame({
                "Pclass": [pclass]*3,
                "Name": [24]*3,
                "Sex": [0]*3, 
                "Age": [19]*3,
                "SibSp": [0]*3,
                "Parch": [0]*3,
                "Fare": [fare]*3,
                "Embarked": ["S", "Q", "C"]
            })
            my_df = pd.get_dummies(my_df, columns=["Embarked"], prefix="Embarked") #делаем one-hot

            # Ensure all dummy columns exist and are integers
            for col in ["Embarked_S", "Embarked_C", "Embarked_Q"]:
                if col not in my_df.columns:
                    my_df[col] = 0
                my_df[col] = my_df[col].astype(int)

            knn_pred = knn.predict(my_df)
            mlp_pred = mlp.predict(my_df)

            # Store object (dict) into a single cell safely
            np_df.at[fare, pclass] = {
                "knn": knn_pred.tolist() if hasattr(knn_pred, "tolist") else knn_pred,
                "mlp": mlp_pred.tolist() if hasattr(mlp_pred, "tolist") else mlp_pred
            }
print(np_df)