def soma_quadrados(num):
    quadrados = []
    for i in range(1, int(num**0.5) + 1):
        quadrados.append(i*i)
    
    resultados = []
    
    for a in quadrados:
        for b in quadrados:
            for c in quadrados:
                for d in quadrados:
                    if a + b + c + d == num:
                        resultados.append([int(a**0.5), int(b**0.5), int(c**0.5), int(d**0.5)])

    return resultados

# Testa números de 1 a 100
for n in range(1, 101):
    result = soma_quadrados(n)
    if result:
        print(f"\nNúmero {n} pode ser expresso como soma de quatro quadrados não nulos:")
        for r in result:
            print(f"{n} = {r[0]}² + {r[1]}² + {r[2]}² + {r[3]}²")