from langchain_ollama.chat_models import Ollama
from langchain_core.messages import Message

import json

with open("Vuori_Final_Approval_2024_09_24.json", "r") as file:
    shap_values_json = json.load(file).get("shap_values")
with open("system.prompt", "r") as file:
    sys_prompt = file.read().strip()

messages = [
    Message(role="system", content=sys_prompt),
    Message(role="user", content=shap_values_json),
]

client = Ollama(model="llama3.2", base_url="http://localhost:11434")
response = client(messages)
print(response.content)