import os
import time
import undetected_chromedriver as uc

# Get the directory of the current script
script_dir = os.path.dirname(os.path.abspath(__file__))

# Construct the relative path to the chromedriver
chromedriver_path = os.path.join(script_dir, "chromedriver-win64", "chromedriver.exe")

options = uc.ChromeOptions()
# Correct the binary location to point to the Chrome browser executable, not the chromedriver
# Assuming the Chrome browser executable is in the same directory as the chromedriver
options.binary_location = os.path.join(script_dir, "chrome-win64", "chrome.exe")
print("wde")
with uc.Chrome(use_subprocess=True, options=options) as driver:
    print("wde")
    driver.get("https://lmarena.ai/")
    print("wde")
    # create an instance of ChromeOptions for undetected_chromedriver
    # initialize the undetected Chrome driver with specified options
    time.sleep(10)
import time

options = uc.ChromeOptions()
# Correct the binary location to point to the Chrome browser executable, not the chromedriver
# Assuming the Chrome browser executable is in the specified directory
options.binary_location = r"C:\Programming\Test\IP_Test\chrome-win64\chrome.exe"
print("wde")
with uc.Chrome(use_subprocess=True, options=options) as driver:
    print("wde")
    driver.get("https://lmarena.ai/")
    print("wde")
    # create an instance of ChromeOptions for undetected_chromedriver
    # initialize the undetected Chrome driver with specified options
    time.sleep(10)