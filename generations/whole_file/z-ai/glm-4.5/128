python
from main13 import knn, mlp
import pandas as pd

np_df = pd.DataFrame(index=range(10, 200, 10), columns=[1, 2, 3])
for pclass in [1, 2, 3]:
    for fare in range(10, 200, 10):
            my_df = pd.DataFrame({
                "Pclass": [pclass]*3,
                "Name": [24]*3,
                "Sex": [0]*3, 
                "Age": [19]*3,
                "SibSp": [0]*3,
                "Parch": [0]*3,
                "Fare": [fare]*3,
                "Embarked": ["S", "Q", "C"]
            })
            my_df = pd.get_dummies(my_df, columns=["Embarked"], prefix="Embarked") # doing one-hot
            my_df["Embarked_S"] = my_df["Embarked_S"].map({True: 1, False: 0})
            my_df["Embarked_C"] = my_df["Embarked_C"].map({True: 1, False: 0})
            my_df["Embarked_Q"] = my_df["Embarked_Q"].map({True: 1, False: 0})

            knn_pred = knn.predict(my_df)
            mlp_pred = mlp.predict(my_df)
            np_df.loc[fare, pclass] = (knn_pred, mlp_pred)
print(np_df)