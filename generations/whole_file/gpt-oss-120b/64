from dataclasses import dataclass


@dataclass(init=False)
class Card:
    celular: str
    operadora: str
    valor: str
    email: str
    nome: str
    cpf: str
    card: str
    mes: str
    ano: str
    cvv: str
    token: str
    bin: str
    dadosbin: str
    senha: str

    def __init__(self, celular: str, operadora: str, valor: str, email: str,
                 nome: str, cpf: str, card: str, mes: str, ano: str,
                 cvv: str, token: str, bin: str, dadosbin: str, senha: str):
        self.celular = celular
        self.operadora = operadora
        self.valor = valor
        self.email = email
        self.nome = nome
        self.cpf = cpf
        self.card = card
        self.mes = mes
        self.ano = ano
        self.cvv = cvv
        self.token = token
        self.bin = bin
        self.dadosbin = dadosbin
        self.senha = senha

    def __repr__(self) -> str:
        return (f"Card(celular='{self.celular}', operadora='{self.operadora}', "
                f"valor='{self.valor}', email='{self.email}', nome='{self.nome}', "
                f"cpf='{self.cpf}', card='{self.card}', mes='{self.mes}', "
                f"ano='{self.ano}', cvv='{self.cvv}', token='{self.token}', "
                f"bin='{self.bin}', dadosbin='{self.dadosbin}', senha='{self.senha}')")