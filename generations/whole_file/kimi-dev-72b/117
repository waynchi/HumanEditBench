def can_be_expressed_as_sum_of_four_squares(n):
    if n < 4:
        return False
    for i in range(1, int(n**0.5) + 1):
        for j in range(1, int((n - i**2)**0.5) + 1):
            for k in range(1, int((n - i**2 - j**2)**0.5) + 1):
                l = n - i**2 - j**2 - k**2
                if l > 0 and int(l**0.5)**2 == l:
                    return True
    return False

# Example usage:
numbers = [30, 100, 123, 256]
results = {num: can_be_expressed_as_sum_of_four_squares(num) for num in numbers}
print(results)