cwd = os.getcwd()
db_path = cwd + '/milvus_db.db'
TABLE_NAME = 'test_table'
DIM_VALUE = 128

# Use pymilvus to create a database connection
connections.connect("default", uri="http://localhost:19530")
client = connections.get_connection("default")