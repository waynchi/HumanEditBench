def is_sum_of_four_non_zero_squares(n):
    """
    检查一个数是否可以表示为四个非零平方数之和。
    """
    if n < 4:
        return False
    for i in range(1, int(n**0.5) + 1):
        for j in range(i, int((n - i**2)**0.5) + 1):
            for k in range(j, int((n - i**2 - j**2)**0.5) + 1):
                l = n - i**2 - j**2 - k**2
                if l > 0 and int(l**0.5)**2 == l:
                    return True
    return False

# 测试示例
print(is_sum_of_four_non_zero_squares(30))  # 输出: True
print(is_sum_of_four_non_zero_squares(10))  # 输出: False