import math

def is_perfect_square(x):
    root = math.isqrt(x)
    return root * root == x

def can_be_expressed_as_sum_of_four_non_zero_squares(n):
    max_val = int(math.isqrt(n))
    for a in range(1, max_val + 1):
        for b in range(1, max_val + 1):
            for c in range(1, max_val + 1):
                d_squared = n - a*a - b*b - c*c
                if d_squared < 1:
                    continue
                if is_perfect_square(d_squared):
                    return True
    return False

# Example: Print all numbers between 1 and 100 that can be expressed
for number in range(1, 100):
    if can_be_expressed_as_sum_of_four_non_zero_squares(number):
        print(number)