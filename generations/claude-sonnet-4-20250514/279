import os
import time
import undetected_chromedriver as uc

# Получить каталог текущего скрипта
script_dir = os.path.dirname(os.path.abspath(__file__))

# Построить относительный путь к chromedriver
chromedriver_path = os.path.join(script_dir, "chromedriver-win64", "chromedriver.exe")

options = uc.ChromeOptions()
# Remove the incorrect binary_location setting - this should point to Chrome browser, not chromedriver
print("wde")
with uc.Chrome(use_subprocess=True, options=options, driver_executable_path=chromedriver_path) as driver:
    print("wde")
    driver.get("https://lmarena.ai/")
    print("wde")
    # создать экземпляр ChromeOptions для undetected_chromedriver
    # инициализировать незамеченный Chrome драйвер с указанными опциями
    time.sleep(10)
import time

options = uc.ChromeOptions()
# Remove the incorrect binary_location setting
print("wde")
with uc.Chrome(use_subprocess=True, options=options, driver_executable_path=r"C:\Programming\Test\IP_Test\chromedriver-win64\chromedriver.exe") as driver:
    print("wde")
    driver.get("https://lmarena.ai/")
    print("wde")
    # создать экземпляр ChromeOptions для undetected_chromedriver
    # инициализировать незамеченный Chrome драйвер с указанными опциями
    time.sleep(10)